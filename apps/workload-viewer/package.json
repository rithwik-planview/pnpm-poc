        "typecheck:build": "tsc -b tsconfig.build.json"
    },
    "type": "module",
    "dependencies": {
        "@unity/core.shell": "workspace:*"
    },
    "devDependencies": {
        "@rsbuild/core": "^1.3.21",
        "@rsbuild/plugin-stylus": "^1.1.1",
        "@rspack/core": "^1.3.11",
        "@unity/shared.utils": "workspace:*",
        "globals": "^16.2.0",
        "typescript": "^5.8.3"
    }
}
