{
    "name": "@unity/apps.workload-viewer",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rsbuild build",
        "dev": "rsbuild dev --port 3000 --open",
        "format": "prettier --write .",
        "lint": "eslint .",
        "mf-dev": "rsbuild mf-dev --open",
        "preview": "rsbuild preview",
        "typecheck": "pnpm typecheck:all && pnpm typecheck:build",
