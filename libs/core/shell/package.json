{
    "name": "@unity/core.shell",
    "version": "1.0.0",
    "dependencies": {
        "@module-federation/enhanced": "^0.13.1",
        "@unity/shared.utils": "workspace:*"
    },
    "devDependencies": {
        "@module-federation/rsbuild-plugin": "^0.13.1",
        "@rsbuild/plugin-react": "^1.3.1",
        "@rslib/core": "^0.7.1",
        "@types/react": "^18.3.21",
        "@types/react-dom": "^18.3.7",
        "globals": "^16.1.0",
        "react": "^19.1.0",
        "react-dom": "^18.3.1",
        "typescript": "^5.8.3"
    },
    "exports": {
        ".": {
            "types": "./dist/esm/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "private": true,
    "scripts": {
        "build": "rslib build",
        "dev": "rslib build --watch",
        "format": "prettier --write .",
        "lint": "eslint .",
        "typecheck": "pnpm typecheck:all && pnpm typecheck:build",
        "typecheck:all": "tsc -b tsconfig.json",
        "typecheck:build": "tsc -b tsconfig.build.json"
    },
    "type": "module",
    "types": "./dist/esm/index.d.ts"
}
