{
    "name": "@unity/features.workplan-import",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rslib build",
        "deploy-package": "deploy-package @unity/features.workplan-import --with-deps",
        "deploy-package:ci": "deploy-package @unity/features.workplan-import",
        "dev": "rslib build --watch",
        "format": "prettier --write .",
        "i18n": "pnpm i18n:extract && pnpm i18n:compile",
        "i18n:compile": "i18n-compile @unity/features.workplan-import",
        "i18n:extract": "i18n-extract @unity/features.workplan-import",
        "lint": "eslint .",
        "mf-dev": "rslib mf-dev",
        "test": "jest",
        "test:ci": "jest --coverage",
        "typecheck": "pnpm typecheck:all && pnpm typecheck:build",
        "typecheck:all": "tsc -b tsconfig.json",
        "typecheck:build": "tsc -b tsconfig.build.json"
    },
    "type": "module",
    "dependencies": {
        "@planview/pv-details": "^1.6.0",
        "@planview/pv-grid": "^1.34.2",
        "@planview/pv-icons": "^2.25.0",
        "@planview/pv-toolbar": "^1.14.1",
        "@planview/pv-uikit": "^7.36.0",
        "@planview/pv-utilities": "^4.22.2",
        "@tanstack/react-query": "^5.77.0",
        "@tanstack/react-query-devtools": "^5.77.0",
        "@unity/core.shell": "workspace:*",
        "@unity/shared.hybrid": "workspace:*",
        "@unity/shared.ui": "workspace:*",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-intl": "^7.1.11",
        "styled-components": "^6.1.18"
    },
    "devDependencies": {
        "@module-federation/rsbuild-plugin": "^0.14.0",
        "@rsbuild/plugin-react": "^1.3.1",
        "@rslib/core": "^0.8.0",
        "@rspack/core": "^1.3.11",
        "@swc/plugin-formatjs": "^3.2.2",
        "@swc/plugin-styled-components": "^7.1.5",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@unity/shared.utils": "workspace:*",
        "@unity/tools": "workspace:*",
        "dotenv": "^16.5.0"
    }
}
