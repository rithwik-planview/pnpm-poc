    "devDependencies": {
        "@hey-api/openapi-ts": "^0.67.6",
        "@rsbuild/plugin-react": "^1.3.1",
        "@rslib/core": "^0.8.0",
        "@rspack/core": "^1.3.11",
        "@types/lodash.debounce": "^4.0.9",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@unity/shared.ui": "workspace:*",
        "@unity/tools": "workspace:*",
        "react": "^19.1.0"
    },
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
