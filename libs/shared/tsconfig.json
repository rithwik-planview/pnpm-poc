{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "ES6",
        "lib": ["esnext", "dom", "DOM.Iterable"],
        "module": "ESNext",
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "moduleResolution": "bundler",
        "useDefineForClassFields": true,
        "allowImportingTsExtensions": true
    },
    "include": ["src"]
}
