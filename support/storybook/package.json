        "@storybook/react-vite": "^9.0.4",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^4.4.1",
        "eslint": "^9.27.0",
        "eslint-plugin-react-hooks": "6.0.0-rc.1",
        "eslint-plugin-react-refresh": "^0.4.19",
        "eslint-plugin-storybook": "^9.0.4",
        "globals": "^16.1.0",
        "storybook": "^9.0.4",
