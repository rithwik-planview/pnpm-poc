{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "rootDir": "./",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@unity/libs/*": ["./libs/*"]
        }
    }
}
